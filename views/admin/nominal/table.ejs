<div class="card-body">
  <table id="example2" class="table table-bordered table-hover">
    <thead>
      <tr>
        <th>No.</th>
        <th>Nama Koin</th>
        <th>Jumlah Koin</th>
        <th>Harga</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% for(let i =0; i<nominal.length; i++) { %>
          <td><%- i+1 %></td>
          <td><%= nominal[i].cointName %></td>
          <td><%= nominal[i].cointQty %></td>
          <td><%= nominal[i].price %></td>
          <td>
            <form action="nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
                <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning">
                    <i class="fas fa-pencil-alt mr-1"></i>
                    Update
                </a>
                <button class="btn btn-danger">
                    <i class="fas fa-trash mr-1"></i>    
                    Delete
                </button>
            </form>
          </td>
      </tr>
      <% } %>

    </tfoot>
  </table>
</div>



